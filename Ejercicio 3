        "def calcular_mcd(a, b):\n",
        "    while b != 0:\n",
        "        a, b = b, a % b\n",
        "    return a\n",
        "\n",
        "num1 = int(input(\"Ingrese el primer número: \"))\n",
        "num2 = int(input(\"Ingrese el segundo número: \"))\n",
        "\n",
        "mcd = calcular_mcd(num1, num2)\n",
        "print(\"El Máximo Común Divisor de los dos números es:\", mcd)\n"
      ]
    }
  ]
